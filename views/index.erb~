  <ul>
	  <li><%=h x.title %></li>
	  <li><%=h x.body %></li>
	  <li><%=h x.created_at %></li>
	  <li>
	  <form action="/delete/<%= x.id %>" method="Post">
           <input name="_method" type="hidden" value="delete"/>
	   <button type="submit" name="ok">Delete</button>
	  </form>
	  </li>
  </ul>
<% end %>
