       <table>
         <thead>
           <tr>
             <th class="show-for-large-only" >ID:</th>
	         <th>Title:</th>
	         <th class="show-for-large-only">FB_ID:</th>
	         <th>Actions</th>
	       </tr> 
         </thead>
         <tbody>
	     <% @cut.each do |x| %>
		 <tr>
           <td class="show-for-large-only"  > <%= x.id %> </td> 
	       <td> <%= x.title %></td>
	       <td class="show-for-large-only"> <%= x.body %></td>
	       <td><a href="/logged_in/delete/<%= x.id %>" class="button alert" >Delete</a><a href="/logged_in/update/<%= x.id %>" class="button" >Update</a></td>
         </tr>    
       <% end %>
	 </tbody>
   </table>

